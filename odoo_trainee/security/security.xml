<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_student" model="ir.module.category">
        <field name="name">student</field>
        <field name="description">category for student</field>
        <field name="sequence">45</field>
    </record>
    <record id="group_gender_male" model="res.groups">
        <field name="name">Male</field>
        <field name="category_id" ref="module_category_student"/>

    </record>
    <record id="group_gender_female" model="res.groups">
        <field name="name">Female</field>
        <field name="category_id" ref="module_category_student"/>
        <!-- <field name="implied_ids" eval="[(4,ref('group_gender_male'))]"/> -->
    </record>
    <record id="module_category_user" model="ir.module.category">
        <field name="name">student_user</field>
        <field name="description">category for student user</field>
        <field name="sequence">47</field>
    </record>
    <record id="group_user_detail" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_user"/>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
    </record>
    <record id="group_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="module_category_user"/>
        <field name="implied_ids" eval="[(4,ref('group_user_detail'))]"/>
    </record>

</odoo>
