<odoo>
    <record id="student_tree" model="ir.ui.view">
        <field name="name">student.tree</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <tree string="strings">
                <field name="enrollmentNo"/>
                <field name="name"/>
                <field name="birthdate" />
                <field name="age" />
                <field name="email"/>
                <field name="gender" />
                <field name="contactNo"/>
                <field name="bloodGroup" />
                <field name="college_ids"/>
                <field name="hobbies_ids" widget="many2many_tags"/>
                <field name="city" />
                <field name="country" />
                <field name="branch"/>
                <field name="bloodGroup" />
                <field name="height" />
                <field name="weight" />
                <field name="pincode" />
                <field name="disabled" />
            </tree>
        </field>
    </record>
    <record id="student_form" model="ir.ui.view">
        <field name="name">student.form</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <form>
                <sheet string="students">
                    <field name="image" widget="image" class="oe_avatar" />
                    <div class="oe_title">
                        <h1>
                            <field name="name" style="width:25%;"/>
                        </h1>
                    </div>
                    <notebook>
                        <page name="page_personal" string="Personal Details">
                            <group>
                                <field name="enrollmentNo" style="width:25%;"/>
                                <field name="birthdate" style="width:25%;"/>
                                <field name="age" style="width:25%;"/>
                                <field name="gender" style="width:25%;"/>
                                <field name="hobbies_ids" widget="many2many_tags" style="width:25%;"/>
                            </group>
                        </page>
                        <page name="page_contact" string="Contact Details">
                            <group>
                                <field name="contactNo" style="width:25%;"/>
                                <field name="email" style="width:25%;"/>
                                <field name="address" style="width:25%;"/>
                                <field name="city" style="width:25%;"/>
                                <field name="pincode" style="width:25%;"/>
                                <field name="country" style="width:25%;"/>
                            </group>
                        </page>
                        <page name="page_other" string="Medical Details">
                            <group>
                                <field name="bloodGroup" style="width:25%;"/>
                                <field name="height" style="width:25%;"/>
                                <field name="weight" style="width:25%;"/>
                                <field name="disabled" style="width:25%;"/>
                            </group>
                        </page>
                        <page name="page_academic" string="Academic Details">
                            <group>
                                <field name="college_ids" style="width:25%;" />
                                <field name="branch" style="width:25%;"/>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    <record id="student_calendar" model="ir.ui.view">
        <field name="name">student.calendar</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <calendar string="students" date_start="birthdate" color="name">
                <field name="name"/>
                <field name="branch" />
                <field name="email" />
            </calendar>
        </field>
    </record>
    <record id="student_graph" model="ir.ui.view">
        <field name="name">student.graph</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <graph string="students">
                <field name="name"/>
                <field name="enrollmentNo" type="measure"/>
            </graph>
        </field>
    </record>
    <record id="student_pivot" model="ir.ui.view">
        <field name="name">student.pivot</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <pivot string="students">
                <field name="name" type="row"/>
                <field name="enrollmentNo" type="measure"/>
            </pivot>
        </field>
    </record>
    <record id="student_search" model="ir.ui.view">
        <field name="name">student.search</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <search string="students">
                <field name="name" />
                <field name="enrollmentNo" />
                <field name="email" />
                <field name="college_ids" />
                <group expand="1" string="Group By">
                    <filter name="group_branch" string="Branch" context="{'group_by': 'branch'}" />
                    <separator />
                    <filter name="group_city" string="City" context="{'group_by': 'city'}"/>
                    <separator />
                    <filter name="group_country" string="Country" context="{'group_by': 'country'}"/>
                </group>
                <filter name="filter_male" string="Male" domain="[('gender', '=', 'male')]" />
                <separator />
                <filter name="filter_female" string="Female" domain="[('gender', '=', 'female')]" />
                <searchpanel>
                    <field name="gender" />
                </searchpanel>
            </search>
        </field>
    </record>
    <record id="student_kanban" model="ir.ui.view">
        <field name="name">Student.kanban</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <kanban>
                <field name="id" />
                <field name="name" />
                <field name="email" />
                <field name="city" />
                <field name="birthdate" />
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div class="o_kanban_image">
                                <img t-att-src="kanban_image('student', 'image', record.id.raw_value)" class="oe_kanban_image" alt="Avatar"/>
                            </div>
                            <div class="row">
                                <div class="oe_kanban_details">
                                    <strong class="o_kanban_record_title" > Name: </strong>
                                    <field name="name" />
                                </div>
                                <div class="oe_kanban_details">
                                    <strong class="o_kanban_record_title" > Date of Birth: </strong>
                                    <field name="birthdate" />
                                </div>
                                <div class="oe_kanban_details">
                                    <strong class="o_kanban_record_title" > Email: </strong>
                                    <field name="email" />
                                </div>
                                <div class="oe_kanban_details">
                                    <strong class="o_kanban_record_title" > City: </strong>
                                    <field name="city" />
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>
    <record id="student_gantt" model="ir.ui.view">
        <field name="name">student.gantt</field>
        <field name="model">student</field>
        <field name="arch" type="xml">
            <gantt string="students" date_start="birthdate" date_stop="dateTo" default_group_by="gender">
                <field name="name"/>
                <field name="enrollmentNo"/>
            </gantt>
        </field>
    </record>
    <record id="college_tree" model="ir.ui.view">
        <field name="name">college.tree</field>
        <field name="model">college</field>
        <field name="arch" type="xml">
            <tree string="Colleges">
                <field name="collegeName"/>
                <field name="collegeCity"/>
            </tree>
        </field>
    </record>
    <record id="college_form" model="ir.ui.view">
        <field name="name">college.form</field>
        <field name="model">college</field>
        <field name="arch" type="xml">
            <form string="Colleges">
                <sheet string="Colleges">
                    <group>
                        <field name="collegeName"/>
                        <field name="collegeCity"/>
                    </group>
                    <group>
                        <field name="student_ids">
                            <tree width="175">
                                <field name="enrollmentNo"/>
                                <field name="name"/>
                                <field name="birthdate" />
                                <field name="age" />
                                <field name="gender" />
                                <field name="email"/>
                                <field name="city" />
                                <field name="hobbies_ids" widget="many2many_tags"/>
                            </tree>
                        </field>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="student_action" model="ir.actions.act_window">
        <field name="name">student</field>
        <field name="res_model">student</field>
        <field name="view_mode">tree,form,calendar,graph,gantt,pivot,search,kanban</field>
    </record>
    <record id="college_action" model="ir.actions.act_window">
        <field name="name">college</field>
        <field name="res_model">college</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="student_menu" name="Student Data" />
    <menuitem id="student" name="Student Details" sequence="1" action="student_action" parent="student_menu"/>
    <menuitem id="college" name="College Details" sequence="2" action="college_action" parent="student_menu"/>
</odoo>