<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record model="ir.ui.view" id="student_tree_view">
            <field name="name">Student</field>
            <field name="model">student</field>
            <field name="arch" type="xml">
               <tree>
               	   <field name="name"/>
               	   <field name="enrollmentNo"/>
               	   <field name="contactNo"/>
               	   <field name="age"/>
               	   <field name="branch"/>	
                   <field name="fees"/>
               </tree>
            </field>
  </record>

  <record model="ir.ui.view" id="student_form_view">
      <field name="name">Student</field>
      <field name="model">student</field>
      <field name="arch" type="xml">
       <form>
         <sheet>
           <div class="oe_title">
             <h1>
               <field name="name"/>
             </h1>
           </div>
           <group>
             <field name="enrollmentNo"/>
             <field name="contactNo"/>
             <field name="age"/>
             <field name="branch"/>
             <field name="fees"/> 
           </group>   
         </sheet>
       </form>      
      </field>
  </record>         

    <record id="student_action" model="ir.actions.act_window" >
        <field name="name">student</field>
        <field name="res_model">student</field>
        <field name="view_mode">tree,form</field>
    </record>
    
    <menuitem id="student_menu" name="Student Data" web_icon="student_demo,static/description/icon.png"/>


    <menuitem id="student" name="Details" sequence="1" action="student_action" parent="student_menu"/>


</odoo>